{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "nodenext",
    "declaration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "dist/"
  },
  "lib": ["es2015", "es2016"],
  "include": [
    "openai.ts"
  ],
  "exclude": [
    "node_modules",
  ],
}
